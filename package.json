{
  "name": "etd-backend",
  "version": "1.0.0",
  "description": "Backend server for ETD App",
  "main": "src/server.js",
  "scripts": {
    "start:dev": "nodemon --exec babel-node src/server.js",
    "build": "babel src -d dist",
    "start": "node dist/server.js",
    "heroku-postbuild": "npm run build",
    "migrate": "sequelize db:migrate",
    "migrate:undo": "sequelize db:migrate:undo",
    "migrate:refresh": "sequelize db:migrate:undo:all",
    "migrate:create": "sequelize migration:generate --name"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/valenstical/etd-backend.git"
  },
  "keywords": [
    "nodejs",
    "express",
    "postgresql",
    "sequelize"
  ],
  "author": "Valentine Okosun",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/valenstical/etd-backend/issues"
  },
  "homepage": "https://github.com/valenstical/etd-backend#readme",
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.17.1",
    "express-validator": "^5.3.1",
    "handlebars": "^4.1.2",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "multer": "^1.4.1",
    "nodemailer": "^6.2.1",
    "pdfcrowd": "^4.9.0",
    "pg": "^7.11.0",
    "pg-hstore": "^2.3.3",
    "pingmydyno": "^1.5.1",
    "random-int": "^2.0.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "sequelize": "^5.8.7",
    "sequelize-cli": "^5.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/register": "^7.4.4",
    "eslint": "^5.9.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "nodemon": "^1.19.1"
  }
}
